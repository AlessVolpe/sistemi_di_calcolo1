#include <stdlib.h>
#include "e1.h"

void list_concat(node_t **l1, node_t *l2) {
	node_t *b = *l1;	 	//node_t *p = *l1;
	node_t *a = l2;
    	if(b =! NULL) goto E;		//if (p==NULL) *l1 = l2;
	*b = *a;    	
E:					//else {
        while(b->next == NULL) goto F;	//while (p->next!=NULL) p = p->next;
        b = b->next;
F:	b->next = *a;			//p -> next = l2;
    					//}
}
