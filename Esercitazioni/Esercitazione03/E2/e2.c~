int counteq(short* v1, short* v2, int n) {
    	short* di =v1; //int i, cnt = 0;
    	short* si = v2; //for (i=0; i<n; ++i) cnt += (v1[i]==v2[i]);
    	int d = n; //return cnt;
	int a = 0;
	d--;
L:	if(d < 0) goto E;
	short c = di[d];
	short b = si[d];
	char cl = c == b ? 1 : 0;
	int cc = (int)cl;
	a = a + cc;
	goto L;
E:	return a;
}
