#include <stdlib.h>
#include <string.h>

void* clone(const void* src, int n) {
    	const void* si = src; 	//void* des = malloc(n);
    	int b = n;		//if (des==0) return 0;
    	void* a = malloc(n);	//memcpy(des, src, n);
    	void* di = a;		//return des;
	if(di == 0) goto N;
	memcpy(di,si,b);
	a = di;
	return a;
N:	a = 0;
	return a;
}
